using System;
using System.Collections.Generic;
using System.Linq;
using System.Text.RegularExpressions;

public class Addition_Of_Strings
	{
		public static void Main(string[] args)
		{
			string x,y,SUM;
			x=Console.ReadLine();
			y=Console.ReadLine();
			Console.WriteLine(x);
			Console.WriteLine(y);
			SUM=x+y;
			Console.WriteLine("Sum = "+SUM);
		}
	}
